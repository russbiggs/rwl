{
    "name": "rwl",
    "version": "1.2.0",
    "description": "parse rwl text to JSON",
    "main": "./dist/rwl-cjs.js",
    "module": "./dist/rwl.mjs",
    "scripts": {
        "build": "rm -rf dist/* && rollup -c && npm run compress-iife && npm run create-compat && npm run compress-amd",
        "compress-iife": "uglifyjs --compress --mangle -o dist/rwl-iife.min.js dist/rwl-iife.js",
        "create-compat": "babel dist/rwl-iife.js | uglifyjs --compress --mangle > dist/rwl-iife-compat.min.js",
        "compress-amd": "uglifyjs --compress --mangle -o dist/rwl-amd.min.js dist/rwl-amd.js",
        "pretest": "npm run build",
        "test": "tape 'test/*-test.js'",
        "coverage": "istanbul cover tape ./test/*-test.js",
        "prepublishOnly": "npm run test"
    },
    "keywords": [
        "rwl",
        "tucson",
        "dendrochronology",
        "JSON"
    ],
    "author": {
        "name": "Russ Biggs",
        "url": "https://russbiggs.com"
    },
    "homepage": "https://github.com/russbiggs/rwl",
    "license": "ISC",
    "bin": {
        "rwl2json": "./bin/rwl2json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/russbiggs/rwl.git"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "istanbul": "^0.4.5",
        "rewire": "^4.0.1",
        "rollup": "^0.57.1",
        "tape": "^4.9.0",
        "uglify-es": "^3.3.9"
    },
    "dependencies": {
        "commander": "^2.19.0",
        "randomatic": "^3.1.1"
    }
}
